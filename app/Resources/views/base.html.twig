<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="utf-8">
        <title>{% block title %}{% endblock %} - Nos amis les oiseaux</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    {% block stylesheets %}
        <!-- Font Amiko -->
        <link href="https://fonts.googleapis.com/css?family=Amiko" rel="stylesheet">
        <!-- Font Roboto -->
        <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
        <!-- Font Titillium web -->
        <link href="https://fonts.googleapis.com/css?family=Titillium+Web" rel="stylesheet">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.0/css/font-awesome.min.css">
        <!-- Bootstrap core CSS -->
        <link href="{{ asset('css/global/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Material Design Bootstrap -->
        <link href="{{ asset('css/global/mdb.min.css') }}" rel="stylesheet">
        <!-- Preloader -->
        <link href="{{ asset('css/global/preloader.css') }}" rel="stylesheet">
        <!-- Footer -->
        <link href="{{ asset('css/global/footer.css') }}" rel="stylesheet">

    {% endblock %}

  </head>

    <body>
    <div id="preloader" style="display: none;">
        <div class="verticalcenter">
            <div class="text-center">
                <div id="status"> <div class="preloader-wrapper big active">
                        <div class="preloader-wrapper big active">
                            <div class="preloader-wrapper big active">
                                <div class="preloader-wrapper big active">
                                    <div class="spinner-layer spinner-blue-only">
                                        <div class="circle-clipper left">
                                            <div class="circle"></div>
                                        </div><div class="gap-patch">
                                            <div class="circle"></div>
                                        </div><div class="circle-clipper right">
                                            <div class="circle"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div></div>
            </div>
        </div>
    </div>
    {% block body %}{% block container %}{% endblock %}{% endblock %}
    {% block sidebar %}{% endblock %}



    {% block javascripts %}
        <!-- JQuery -->
        <script type="text/javascript" src="{{ asset('js/jquery-3.1.1.min.js') }}"></script>

        <!-- Bootstrap tooltips -->
        <script type="text/javascript" src="{{ asset('js/tether.min.js') }}"></script>

        <!-- Bootstrap core JavaScript -->
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}"></script>

        <!-- MDB core JavaScript -->
        <script type="text/javascript" src="{{ asset('js/mdb.min.js') }}"></script>
        <!-- js local -->
        <script src="{{ asset('js/main.js') }}"></script>

    {% endblock %}
</body>

</html>
