
{% if picture is not null %}
    <img src="../{{ vich_uploader_asset(picture, 'imageFile') }}" alt="{{ picture.imageName }}" style="width: 110px; height: 130px;"/>
{% else %}
    <img src="{{ absolute_url(asset('img/no-photos.png')) }}" alt="Avatar vide" style="width: 110px; height: 130px;"/>
{% endif %}